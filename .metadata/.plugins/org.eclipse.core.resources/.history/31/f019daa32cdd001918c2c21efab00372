package cn.com.agree.springcloud.entities;

import java.io.Serializable;

import lombok.Data;

@Data
public class Dept implements Serializable{
	
	//private static final long serialVersionUID = 341841189363098548L;
	public Long 	deptno; // 主键
	public String 	dname; // 部门名称
	public String 	db_source;// 来自那个数据库，因为微服务架构可以一个服务对应一个数据库，同一个信息被存储到不同数据库
	
}
